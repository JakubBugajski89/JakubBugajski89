function Numbers(max, min) {
    const wynik = Math.ceil(Math.random() * (max - min)) + min;
    return wynik;
}

let x = Numbers(1, 300);
let y = Numbers(1, 300);

console.log(`${x}`);
console.log(`${y}`);

let a = 0;
for (let i = 1; i <= x; i++) {
    if (x % i == 0) {
        a = a + i;
    }
}
console.log(`${a}`);

let b = 0;
for (let j = 1; j <= y; j++) {
    if (y % j == 0)
        b = b + j;
}
console.log(`${b}`);
